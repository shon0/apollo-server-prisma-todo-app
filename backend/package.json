    "@graphql-tools/load": "7.4.1",
    "@graphql-tools/schema": "8.3.1",
    "@prisma/client": "3.4.0",
    "apollo-server": "3.4.0",
    "graphql": "16.0.1",
    "prisma": "3.4.0"
  },
  "devDependencies": {
    "ts-node": "10.4.0",
    "ts-node-dev": "1.1.8"
  }
}
